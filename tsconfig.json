{
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./*"]
    }
  }
}
